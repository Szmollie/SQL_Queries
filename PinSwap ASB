  SELECT "timestamp", unit_id, "testrun_result", 
  "102050" AS "GS13zero",
  "102060" as "GS24zero",
  "102070" AS "GS57zero",
  "102080" as "GS6Rzero",
  "102090" AS "KV1 zero",
  "102100" as "KV2 zero",
  "400200" as "GS13Neutral",
