  "104020" as "KV1End",
  "104070" as "KV2End",
  "station" as "TestStation(L0xS0x)",
  '' as "ChangeTiming"
  From FlatTable.DEB.Advanced."DEB_STATION_DQEOL117_L01S03_Val_ALL"
  Where
  unit_id = 'A2C775864032504170509E' and
  dir0 = '2025' //change to gold run year
  And dir1 = '07' //change to gold run month
  And Dir2 in ('16') //change to gold run date
  
